{
  "name": "ember-aframe",
  "version": "0.5.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "a-frame",
    "ember-addon"
  ],
  "repository": "https://github.com/ember-vr/ember-aframe.git",
  "license": "MIT",
  "author": "Kelly Selden",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "aframe": "^1.0.4",
    "broccoli-file-creator": "^2.0.0",
    "broccoli-merge-trees": "^3.0.0",
    "ember-aframe-shim": "^2.6.6",
    "ember-awesome-macros": "^5.0.1",
    "ember-cli-babel": "^7.18.0",
    "ember-cli-htmlbars": "^4.2.3",
    "ember-cli-string-utils": "^1.1.0",
    "ember-concurrency": "^1.0.0",
    "ember-macro-helpers": "^4.2.2",
    "jsdom": "^15.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "@kellyselden/ember-blueprint": "1.1.2",
    "aframe-event-set-component": "^5.0.0",
    "aframe-layout-component": "^5.3.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-aframe-camera-extras": "^0.5.1",
    "ember-cli": "3.17.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-dependency-lint": "^1.1.3",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-component-inbound-actions": "^1.3.1",
    "ember-controller-lifecycle": "^1.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-fork-me": "^3.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.18.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.4.0",
    "ember-test-selectors": "^3.0.0",
    "ember-transition-helper": "1.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-ember": "^7.13.0",
    "eslint-plugin-node": "^11.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.1.0",
    "renovate-config-standard": "^2.0.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
